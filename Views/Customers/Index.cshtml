@model MovieSubscription.Models.Customers
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customers</h2>

<ul>

    @foreach (var mv in Model.CustomerList)
    {
        <li>@Html.ActionLink(mv.Name, "Details", "Customers", new { id = mv.ID },null)</li>
    }
</ul>

